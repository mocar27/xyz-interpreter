Void function testValue(Integer a, String b) {
    a = a + 1;
    b = "six";
}

Void function testReference(var Integer a, var String b) {
    a = a + 1;
    b = "six";
}

Integer function testReturn(Integer a) {
    a = 500;
    return a;
}

Integer function main() {
    Integer x = 5;
    String five = "five";

    Void function testGlobal() {
        x = x + 1;
        printInt(x);
    }

    Void function testLocal() {
        Integer x = 10;
        testGlobal();
        printInt(x);
    }

    testValue(x, five);
    printInt(x);       // x = 5
    printString(five); // five = "five"

    testReference(x, five);
    printInt(x);       // x = 6
    printString(five); // five = "six"

    x = testReturn(x);
    printInt(x);        // x = 500

    testLocal();
    printInt(x);    // x = 501

    return 0;
}
